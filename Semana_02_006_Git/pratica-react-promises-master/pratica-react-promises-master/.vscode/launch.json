  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "sourceMaps": true,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/index.ts",
      // "program": "${file}",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch js",
      "skipFiles": [
        "<node_internals>/**"
      ],
      // "program": "${workspaceFolder}/src/index.ts",
      "program": "${file}",
    }
  ]
}